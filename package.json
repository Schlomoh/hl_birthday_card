{
  "name": "birthday_card_hl",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "cleanDist" : "rm -dfr dist/images",
    "build": "tsc && vite build && npm run cleanDist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "npx gh-pages -d dist",
    "index_images": "ts-node ./index_images.ts",
    "push": "npm run index_images && git push origin main && npm run deploy"
  },
  "dependencies": {
    "@schlomoh/react-cookieconsent": "^1.6.1",
    "crypto-js": "^4.1.1",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-p5": "^1.3.35",
    "react-responsive-masonry": "^2.1.7",
    "react-router-dom": "^6.12.1",
    "react-use": "^17.4.0",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^20.2.5",
    "@types/p5": "^1.6.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-responsive-masonry": "^2.1.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prop-types": "^15.8.1",
    "sharp": "^0.32.1",
    "styled-components": "^5.3.10",
    "ts-node": "^10.4.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
